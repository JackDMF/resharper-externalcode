<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>ReSharper.ExternalSources</id>
    <version>1.0.0</version>
    <title>External source support for ReSharper</title>
    <authors>Jason Morse</authors>
    <owners>Jason Morse</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <projectUrl>https://github.com/jberd126/resharper-externalsources</projectUrl>
    <licenseUrl>https://raw.github.com/jberd126/resharper-externalsources/master/license.txt</licenseUrl>
    <iconUrl>https://raw.github.com/jberd126/resharper-externalsources/master/icon.png</iconUrl>
    <copyright>Copyright &#x00A9; 2014 Jason Morse</copyright>
    <description>Adds support for specifying external sources in ReSharper.</description>
    <releaseNotes>Initial release.</releaseNotes>
    <dependencies>
      <dependency id="ReSharper" version="8.1" />
    </dependencies>
    <tags>resharper external sources</tags>
  </metadata>
  <files>
    <!-- ReSharper 8.1 -->
    <file src="bin\v8.1\*.dll" target="ReSharper\v8.1\plugins\" />
    <file src="bin\v8.1\*.pdb" target="ReSharper\v8.1\plugins\" />
  </files>
</package>